				([PersonID]
				,[CustomerCode])
		VALUES(@PersonID, @CustomerCode)

	
	INSERT INTO [dbo].[PersonDocument]
			   ([DocumentID]
			   ,[PersonId]
			   ,[DocumentCode])
		 VALUES
			   (@DocumentID, @PersonID, @DocumentCode);

	SELECT @PersonID;
*/
GO
/****** Object:  StoredProcedure [dbo].[spRegionDelete]    Script Date: 2/15/2020 3:52:05 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:		<Author,,Name>
-- Create date: <Create Date,,>
-- Description:	<Description,,>
-- =============================================
CREATE PROCEDURE [dbo].[spRegionDelete]
	@RegionID INT
AS
BEGIN
	SET NOCOUNT ON;

	DELETE FROM [dbo].[Region]
		WHERE [RegionID] = @RegionID;

END
GO
