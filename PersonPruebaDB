-- =============================================
CREATE PROCEDURE [dbo].[spRegionEdit]
	@RegionID INT,
	@RegionName NVARCHAR(100)
AS
BEGIN
	SET NOCOUNT ON;

	UPDATE [dbo].[Region]
		SET [RegionName] = @RegionName
	WHERE [RegionID] = @RegionID
END
GO
