/****** Object:  StoredProcedure [dbo].[spEmployeeEdit]    Script Date: 2/15/2020 3:52:05 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:		<Author,,Name>
-- Create date: <Create Date,,>
-- Description:	<Description,,>
-- =============================================
CREATE PROCEDURE [dbo].[spEmployeeEdit]
	-- Add the parameters for the stored procedure here
	--@DocumentID INT,
	@PersonID INT,
	@EmployeeCode CHAR(6),
	@Lastname NVARCHAR(100),
	@Firstname NVARCHAR(100),
	@User NVARCHAR(100),
	@Password NVARCHAR(100),
	@Gender CHAR(1),
	@Birthday DATE,
	@Pay DECIMAL(18,2),
	@Notes TEXT
AS
BEGIN
	-- SET NOCOUNT ON added to prevent extra result sets from
	-- interfering with SELECT statements.
	SET NOCOUNT ON;

    -- Insert statements for procedure here
	UPDATE [dbo].[Person]
	   SET [Lastname] = @Lastname
		  ,[Firstname] = @Firstname
		  ,[Gender] = @Gender
		  ,[Birthday] = @Birthday
		  ,[Notes] = @Notes
	   WHERE [PersonID] = @PersonID;

	UPDATE [dbo].[Employee]
	   SET [PersonID] = @PersonID
		  ,[EmployeeCode] = @EmployeeCode
		  ,[Pay] = @Pay
		  ,[User] = @User
		  ,[Password] = @Password
		  WHERE [PersonID] = @PersonID;
END
GO
/****** Object:  StoredProcedure [dbo].[spEmployeeGetAll]    Script Date: 2/15/2020 3:52:05 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:		<Author,,Name>
-- Create date: <Create Date,,>
-- Description:	<Description,,>
-- =============================================
CREATE PROCEDURE [dbo].[spEmployeeGetAll]
AS
BEGIN
	-- SET NOCOUNT ON added to prevent extra result sets from
	-- interfering with SELECT statements.
	SET NOCOUNT ON;

    -- Insert statements for procedure here
	SELECT [p].[PersonID]
		  ,[e].[EmployeeCode]
		  ,[p].[Lastname]
		  ,[p].[Firstname]
		  ,[e].[User]
		  ,[e].[Password]
		  ,[p].[Gender]
		  ,[p].[Birthday]
		  ,[e].[Pay]
		  ,[p].[Notes]
	  FROM [dbo].[Employee] AS [e]
	  LEFT JOIN [dbo].[Person] AS [p] 
		ON [p].[PersonID] = [e].[PersonID]
END
GO
/****** Object:  StoredProcedure [dbo].[spEmployeeInsert]    Script Date: 2/15/2020 3:52:05 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:		<Author,,Name>
-- Create date: <Create Date,,>
-- Description:	<Description,,>
-- =============================================
CREATE PROCEDURE [dbo].[spEmployeeInsert]
	-- Add the parameters for the stored procedure here
	--@DocumentID INT,
	@EmployeeCode CHAR(6),
	@Lastname NVARCHAR(100),
	@Firstname NVARCHAR(100),
	@User NVARCHAR(100),
	@Password NVARCHAR(100),
	@Gender CHAR(1),
	@Birthday DATE,
	@Pay DECIMAL(18,2),
	@Notes TEXT
	--@DocumentCode NVARCHAR(10),
AS
BEGIN
	-- SET NOCOUNT ON added to prevent extra result sets from
	-- interfering with SELECT statements.
	SET NOCOUNT ON;

    -- Insert statements for procedure here
	DECLARE @PersonID INT;

	INSERT INTO [dbo].[Person]
			   ([Lastname]
			   ,[Firstname]
			   ,[Gender]
			   ,[Birthday]
			   ,[Notes])
		 VALUES
			   (@Lastname, @Firstname,@Gender, @Birthday, @Notes)
		   SELECT @PersonID = SCOPE_IDENTITY();
		   
	INSERT INTO [dbo].[Employee]
           ([PersonID]
           ,[EmployeeCode]
           ,[Pay]
           ,[User]
           ,[Password])
     VALUES
           (@PersonID
		   ,@EmployeeCode
		   ,@Pay
		   ,@User
		   ,@Password)

		   SELECT @PersonID;
END
GO
