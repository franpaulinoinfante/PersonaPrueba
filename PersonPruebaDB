SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
-- =============================================
-- Author:		<Author,,Name>
-- Create date: <Create Date,,>
-- Description:	<Description,,>
-- =============================================
CREATE PROCEDURE [dbo].[spAddressEdit] --spAddressInsert --spAddressGetAll
	@AddressID INT,
	@PersonID INT,
	@CityID INT,
	@TypeAddressID INT,
	@Sector NVARCHAR(100),
	@Calle NVARCHAR(100),
	@LocalNumber NVARCHAR(5)
AS
BEGIN
	-- SET NOCOUNT ON added to prevent extra result sets from
	-- interfering with SELECT statements.
	SET NOCOUNT ON;

    -- Insert statements for procedure here
	UPDATE [dbo].[Address]
	   SET [PersonaID] = @PersonID
		  ,[CityID] = @CityID
		  ,[TypeAddressID] = @TypeAddressID
		  ,[Sector] = @Sector
		  ,[Calle] = @Calle
		  ,[LocalNumber] = @LocalNumber
	 WHERE [AddressID] = @AddressID;
END
GO
