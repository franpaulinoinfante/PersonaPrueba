CREATE PROCEDURE [dbo].[spRegionInsert]
	@RegionName NVARCHAR(100)
AS
BEGIN
	SET NOCOUNT ON;
	INSERT INTO [dbo].[Region]
           ([RegionName])
     VALUES
           (@RegionName)
	SELECT SCOPE_IDENTITY();
END
GO
